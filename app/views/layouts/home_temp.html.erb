      </ul>      
    </div><!-- /container -->
   
    <script>
      new gnMenu( document.getElementById( 'gn-menu' ) );
    </script>

  <div class = "display">
    <%= yield %> 
  </div>
  <br><br>

  

</div> <!-- /container -->


</body>
</html>