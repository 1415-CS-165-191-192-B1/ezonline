<h1>List of All Snippets</h1><br />
<h2>Snippets</h2>
 
<table>
  <tr>
    <th>Title</th>
  </tr>
 
<% @snippets.each do |snippet| %>
  <tr>
    <td><%= snippet.title %></td>
    <td>
        <%= button_to "Show History",
                      history_file_index_path(snippet),
                      method: :get
        %>
    </td>
  </tr>
<% end %>
</table>
