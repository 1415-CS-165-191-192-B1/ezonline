<h1>Add a Google Doc</h1>

<%= form_for :title, url: file_fetch_path, html: {class: "nifty_form"} do |c| %>
  <%= c.text_field :text, value: 'type in exact title' %>
  <%= c.submit 'Add' %>
<% end %>

<% unless session[:vimeo_access] %>
	<%= button_to "Login to Vimeo", 
				  user_vlogin_path,
				  method: :get, 
				  form_class: "new-thing" %>
<% end %>

<% if session[:video_id] %>
	<% link = "//player.vimeo.com/video/" + session[:video_id] %>
	<iframe src=<%= link %> width="800" height="500" frameborder="0"></iframe>
<% end %>